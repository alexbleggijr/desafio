{
  "name": "desafio",
  "version": "0.0.1",
  "description": "Stencil Component Starter",
  "main": "dist/index.cjs.js",
  "module": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "unpkg": "dist/desafio/desafio.esm.js",
  "exports": {
    ".": {
      "import": "./dist/desafio/desafio.esm.js",
      "require": "./dist/desafio/desafio.cjs.js"
    },
    "./my-component": {
      "import": "./dist/components/my-component.js",
      "types": "./dist/components/my-component.d.ts"
    },
    "./loader": {
      "import": "./loader/index.js",
      "require": "./loader/index.cjs",
      "types": "./loader/index.d.ts"
    }
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "sass:start": "sass src/styles:styles --watch",
    "sass:build": "sass src/styles:styles --no-source-map",
    "stencil:start": "stencil build --dev --watch",
    "stencil:build": "stencil build",
    "stencil:test": "stencil test --spec --e2e",
    "stencil:test:watch": "stencil test --spec --e2e --watchAll",
    "stencil:generate": "stencil generate",
    "storybook:start": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "styles:clean": "shx rm -rf styles/*",
    "start": "npm-run-all styles:clean sass:build --parallel sass:start stencil:start storybook:start"
  },
  "license": "MIT",
  "devDependencies": {
    "@chromatic-com/storybook": "^1.6.1",
    "@stencil/core": "^4.19.2",
    "@stencil/sass": "^3.0.12",
    "@storybook/addon-essentials": "^8.2.2",
    "@storybook/addon-links": "^8.2.2",
    "@storybook/blocks": "^8.2.2",
    "@storybook/test": "^8.2.2",
    "@storybook/web-components": "^8.2.2",
    "@storybook/web-components-vite": "^8.2.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "jest": "^29.7.0",
    "jest-cli": "^29.7.0",
    "lit": "^3.1.4",
    "npm-run-all": "^4.1.5",
    "puppeteer": "^22.13.0",
    "sass": "^1.77.8",
    "shx": "^0.3.4",
    "storybook": "^8.2.2"
  }
}
